{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "src/**/services/**/*.ts",
    "src/**/operators/**/*.ts",
    "src/**/transitions/**/*.ts"
  ],
  "ignoreStatic": true,
  "testRunner": "jest",
  "reporters": [
    "progress",
    "clear-text",
    "html",
    "json"
  ],
  "concurrency": 2,
  "coverageAnalysis": "perTest"
}
